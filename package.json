{
   "name": "PeraSwarm-VR-Visualizer",
   "version": "1.0.0",
   "description": "The GUI visualizor for Pera Swarm Project using VR",
   "private": "false",
   "main": "app.js",
   "engines": {
      "node": ">=8.0.0"
   },
   "scripts": {
      "dev": "run-s dev:js webpack-server",
      "build": "run-s prebuild build:dir build:js",
      "prebuild": "run-s clean lint",
      "clean": "rimraf build",
      "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md)\"",
      "lint": "eslint src/js/",
      "webpack-server": "webpack-dev-server --env.NODE_ENV=dev --progress --color --open --hot",
      "webpack-watch": "webpack --env.NODE_ENV=dev --progress --color --watch --hot",
      "dev:js": "webpack --env.NODE_ENV=dev",
      "build:dir": "copyfiles -a -u 2 src/public/assets/**/*.* build/",
      "build:js": "webpack --env.NODE_ENV=prod --progress --color --verbose",
      "test": "echo \"Error: no test specified\" && exit 1",
      "predeploy": "npm run build",
      "deploy": "gh-pages -d build"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Pera-Swarm/visualizer.git"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/Pera-Swarm/visualizer-vr/issues"
   },
   "homepage": "https://pera-swarm.github.io/visualizer-vr",
   "devDependencies": {
      "@babel/core": "^7.10.4",
      "@babel/preset-env": "^7.10.4",
      "autoprefixer": "^9.8.4",
      "babel-loader": "^8.1.0",
      "cname-webpack-plugin": "^3.0.0",
      "copyfiles": "^2.3.0",
      "css-loader": "^3.6.0",
      "eslint": "^7.4.0",
      "file-loader": "^6.1.1",
      "gh-pages": "^3.1.0",
      "html-webpack-plugin": "^4.3.0",
      "mini-css-extract-plugin": "^0.9.0",
      "node-sass": "^4.14.1",
      "npm-run-all": "^4.1.5",
      "optimize-css-assets-webpack-plugin": "^5.0.3",
      "postcss-loader": "^3.0.0",
      "rimraf": "^3.0.2",
      "sass-loader": "^8.0.2",
      "source-map-loader": "^1.0.1",
      "style-loader": "^1.2.1",
      "terser-webpack-plugin": "^3.0.6",
      "webpack": "^4.43.0",
      "webpack-cli": "^3.3.12",
      "webpack-dev-server": "^3.11.0"
   },
   "dependencies": {
      "@tweenjs/tween.js": "^18.6.0",
      "es6-promise": "^4.2.8",
      "jquery": "^3.5.1",
      "mqtt": "^4.2.1",
      "paho-mqtt": "^1.1.0",
      "prettier": "^2.2.1",
      "three": "^0.118.3",
      "three-stl-loader": "^1.0.6",
      "threear": "^1.0.0-alpha.5",
      "update": "^0.7.4"
   }
}
